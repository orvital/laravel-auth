{
    "name": "orvital/laravel-auth",
    "description": "Laravel auth package.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jona Goldman",
            "email": "jonagoldman@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/auth": "^10.0",
        "illuminate/contracts": "^10.0",
        "illuminate/database": "^10.0",
        "illuminate/notifications": "^10.0",
        "illuminate/support": "^10.0",
        "illuminate/validation": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "orchestra/testbench": "^8.10"
    },
    "autoload": {
        "psr-4": {
            "Orvital\\Auth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orvital\\Auth\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Orvital\\Auth\\AuthServiceProvider"
            ],
            "dont-discover": []
        }
    }
}
